DEFINE TYPE SHOKWDS
    KEYWORD ALL
    KEYWORD REWRITE_RULES
    KEYWORD ALIASES
    KEYWORD PATHS
    KEYWORD SYSTEM_USERS
    KEYWORD LISTS
    KEYWORD LOCAL_DOMAINS
    KEYWORD CONFIGURATION_FILE
    KEYWORD FILE_SERVERS
    KEYWORD MESSAGE_CENTERS
    KEYWORD SMTP
    KEYWORD LOCAL
    KEYWORD MLF
    KEYWORD ROUTER
    KEYWORD DECNET_SMTP
    KEYWORD SITE
    KEYWORD VERSION
    KEYWORD QUEUE, SYNTAX=QUEUE_SHOW
    KEYWORD KEY, SYNTAX=SHOW_KEY
    KEYWORD INSIDE_NETWORK_ADDRESSES
    KEYWORD USERS

DEFINE TYPE PATHKWDS
    KEYWORD LOCAL
    KEYWORD SMTP
    KEYWORD SITE
    KEYWORD DECNET_SMTP
    KEYWORD HOLDING_QUEUE, NONNEGATABLE, VALUE (TYPE=$NUMBER,REQUIRED)

DEFINE TYPE RTOKWDS
    KEYWORD SENDER, NEGATABLE
    KEYWORD LIST, NEGATABLE

DEFINE TYPE PROKWDS
    KEYWORD SYSTEM, NONNEGATABLE, VALUE
    KEYWORD OWNER, NONNEGATABLE, VALUE
    KEYWORD GROUP, NONNEGATABLE, VALUE
    KEYWORD WORLD, NONNEGATABLE, VALUE

DEFINE TYPE STRIPKWDS
    KEYWORD RECEIVED, NEGATABLE
    KEYWORD OTHER, NEGATABLE

DEFINE TYPE LIST_HEADER_TYPES
    KEYWORD SUBSCRIBE, VALUE, NEGATABLE
    KEYWORD UNSUBSCRIBE, VALUE, NEGATABLE
    KEYWORD HELP, VALUE(REQUIRED), NEGATABLE

DEFINE TYPE SETTINGKWDS
    !
    !  Each is NONNEGATABLE with a NO form following so DISALLOW works
    !
    KEYWORD MAIL, NONNEGATABLE
    KEYWORD NOMAIL, NONNEGATABLE
    KEYWORD REPRO, NONNEGATABLE
    KEYWORD NOREPRO, NONNEGATABLE
    KEYWORD CONCEAL, NONNEGATABLE
    KEYWORD NOCONCEAL, NONNEGATABLE
    KEYWORD DIGEST, NONNEGATABLE
    KEYWORD NODIGEST, NONNEGATABLE
    KEYWORD POST, NONNEGATABLE
    KEYWORD NOPOST, NONNEGATABLE
    KEYWORD DEFAULT, NONNEGATABLE

DEFINE TYPE JUNKKWDS
    KEYWORD HIGH, NONNEGATABLE
    KEYWORD MEDIUM, NONNEGATABLE
    KEYWORD LOW, NONNEGATABLE

DEFINE TYPE LSTIGNKWDS
    KEYWORD MISSING_LIST_ADDRESS, NEGATABLE
    KEYWORD JUNK_MAIL, VALUE (TYPE=JUNKKWDS, DEFAULT="MEDIUM"), NEGATABLE

DEFINE TYPE RQCKWDS
    KEYWORD INTERVAL, VALUE (REQUIRED, TYPE=$DELTATIME)

DEFINE TYPE NTFYKWDS
    KEYWORD ADD, NONNEGATABLE
    KEYWORD ALL, NONNEGATABLE
    KEYWORD REMOVE, NONNEGATABLE
    KEYWORD REQUEST, NONNEGATABLE
    KEYWORD SET, NONNEGATABLE

DEFINE TYPE DEFKWDS
    KEYWORD REWRITE_RULE, SYNTAX=DEFREWRITE, NONNEGATABLE
    KEYWORD SYSTEM_USERS, SYNTAX=DEFSYSUCMD, NONNEGATABLE
    KEYWORD PATH, SYNTAX=DEFPATHCMD, NONNEGATABLE
    KEYWORD ALIAS, SYNTAX=DEFALIASCMD, NONNEGATABLE
    KEYWORD LIST, NONNEGATABLE, SYNTAX=DEFLISTCMD
    KEYWORD FILE_SERVER, SYNTAX=DEFFSUCMD, NONNEGATABLE
    KEYWORD LOCAL_DOMAIN, SYNTAX=DEFLCLDOM, NONNEGATABLE
    KEYWORD INSIDE_NETWORK_ADDRESS, SYNTAX=DEFRELAY, NONNEGATABLE

DEFINE TYPE MODKWDS
    KEYWORD REWRITE_RULE, SYNTAX=DEFREWRITE, NONNEGATABLE
    KEYWORD PATH, SYNTAX=MODPATHCMD, NONNEGATABLE
    KEYWORD ALIAS, SYNTAX=DEFALIASCMD, NONNEGATABLE
    KEYWORD LIST, NONNEGATABLE, SYNTAX=DEFLISTCMD
    KEYWORD FILE_SERVER, SYNTAX=DEFFSUCMD, NONNEGATABLE
    KEYWORD INSIDE_NETWORK_ADDRESS, SYNTAX=DEFRELAY, NONNEGATABLE
    KEYWORD USER, SYNTAX=MODUSER, NONNEGATABLE

DEFINE TYPE REMKWDS
    KEYWORD REWRITE_RULE, SYNTAX=REMREWRITE, NONNEGATABLE
    KEYWORD PATH, SYNTAX=REMPATHCMD, NONNEGATABLE
    KEYWORD ALIAS, SYNTAX=REMALIASCMD, NONNEGATABLE
    KEYWORD LIST, NONNEGATABLE, SYNTAX=REMLISTCMD
    KEYWORD FILE_SERVER, NONNEGATABLE, SYNTAX=REMFSUCMD
    KEYWORD LOCAL_DOMAIN, NONNEGATABLE, SYNTAX=REMLCLDOM
    KEYWORD INSIDE_NETWORK_ADDRESS, SYNTAX=REMRELAY, NONNEGATABLE
    KEYWORD USER, SYNTAX=REMUSER, NONNEGATABLE

DEFINE SYNTAX DEFREWRITE
    ROUTINE CMD_DEFREW
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    QUALIFIER REGEX
    PARAMETER P2, LABEL=LHS, PROMPT="Left-hand side", VALUE (REQUIRED)
    PARAMETER P3, LABEL=RHS, PROMPT="Right-hand side", VALUE (REQUIRED)

DEFINE SYNTAX DEFLCLDOM
    ROUTINE CMD_DEFLCLDOM
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=HOSTNM, PROMPT="domain name pattern", VALUE (REQUIRED)

DEFINE SYNTAX DEFRELAY
    ROUTINE CMD_DEFRELAY
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=NETADDR, PROMPT="IP address", VALUE (REQUIRED)
    QUALIFIER NETMASK, VALUE (REQUIRED)
    QUALIFIER REJECT
    QUALIFIER RELAY_ALLOWED
    DISALLOW (REJECT AND RELAY_ALLOWED)

DEFINE SYNTAX DEFSYSUCMD
    ROUTINE CMD_DEFSYSU
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=USERLIST, PROMPT="Address", VALUE (REQUIRED, LIST)

DEFINE SYNTAX DEFPATHCMD
    ROUTINE CMD_DEFPATH
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=DOMAIN, PROMPT="Domain", VALUE (REQUIRED)
    PARAMETER P3, LABEL=PATHNAME, PROMPT="Path", VALUE (REQUIRED, TYPE=PATHKWDS)
    QUALIFIER ROUTE, VALUE (REQUIRED)

DEFINE SYNTAX MODPATHCMD
    ROUTINE CMD_DEFPATH
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=DOMAIN, PROMPT="Domain", VALUE (REQUIRED)
    PARAMETER P3, LABEL=PATHNAME, PROMPT="Path", VALUE (TYPE=PATHKWDS)
    QUALIFIER ROUTE, VALUE (REQUIRED)

DEFINE SYNTAX DEFALIASCMD
    ROUTINE CMD_DEFALIAS
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=LCLNAME, PROMPT="Local name", VALUE (REQUIRED)
    PARAMETER P3, LABEL=ALIASADR, PROMPT="Alias", VALUE (LIST, REQUIRED)

DEFINE SYNTAX DEFLISTCMD
    ROUTINE CMD_DEFLIST
    PARAMETER P1, LABEL=DEFKWD, VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=LISTNAME, VALUE (REQUIRED), PROMPT="List name"
    QUALIFIER OWNER, NONNEGATABLE, VALUE (REQUIRED, LIST)
    QUALIFIER MODERATOR, NEGATABLE, VALUE (REQUIRED, LIST)
    QUALIFIER ERRORS_TO, NONNEGATABLE, VALUE (REQUIRED)
    QUALIFIER REPLY_TO, NONNEGATABLE, VALUE (REQUIRED, LIST, TYPE=RTOKWDS)
    QUALIFIER ARCHIVE, NEGATABLE, VALUE (REQUIRED, TYPE=$FILE)
    QUALIFIER DESCRIPTION, NONNEGATABLE, VALUE (REQUIRED)
    QUALIFIER PROTECTION, NONNEGATABLE, VALUE (REQUIRED, LIST, TYPE=PROKWDS)
    QUALIFIER ADD_MESSAGE, NEGATABLE, VALUE (REQUIRED, TYPE=$FILE)
    QUALIFIER REMOVE_MESSAGE, NEGATABLE, VALUE (REQUIRED, TYPE=$FILE)
    QUALIFIER FORWARD_MESSAGE, NEGATABLE, VALUE (REQUIRED, TYPE=$FILE)
    QUALIFIER RETURN_ADDRESS, NEGATABLE, VALUE (REQUIRED)
    QUALIFIER STRIP_HEADER, NONNEGATABLE, VALUE (REQUIRED, LIST, TYPE=STRIPKWDS)
    QUALIFIER PRIVATE, NEGATABLE
    QUALIFIER CASE_SENSITIVE, NEGATABLE
    QUALIFIER DIGEST, NEGATABLE
    QUALIFIER RECIPIENT_MAXIMUM, NEGATABLE, VALUE (REQUIRED)  ! number or DEFAULT
    QUALIFIER HIDE_ERRORS_TO, NEGATABLE
    QUALIFIER CC_POST_ERRORS, NEGATABLE
    QUALIFIER HOSTNAME, VALUE(REQUIRED), NEGATABLE
    QUALIFIER SUBJECT_PREFIX, VALUE(REQUIRED), NEGATABLE
    QUALIFIER IGNORE, VALUE (REQUIRED, LIST, TYPE=LSTIGNKWDS)
    QUALIFIER LIST_HEADERS, NEGATABLE,
		VALUE (REQUIRED, LIST, TYPE=LIST_HEADER_TYPES)
    QUALIFIER XHEADERS, NEGATABLE, VALUE(REQUIRED, LIST)
    QUALIFIER SETTINGS, NONNEGATABLE, VALUE(REQUIRED, LIST, TYPE=SETTINGKWDS)
    QUALIFIER MAXIMUM_MESSAGE_SIZE, VALUE (REQUIRED, TYPE=$NUMBER)
    QUALIFIER CONFIRMATION_MESSAGE, NEGATABLE, VALUE (REQUIRED, TYPE=$FILE)
    QUALIFIER REQUEST_CONFIRMATION, NEGATABLE, VALUE (TYPE=RQCKWDS)
    QUALIFIER TEXT_ONLY, NEGATABLE
    QUALIFIER NOTIFY, NEGATABLE, VALUE (REQUIRED, LIST, TYPE=NTFYKWDS)
    DISALLOW NOTIFY.ALL AND
                (NOTIFY.ADD OR NOTIFY.REMOVE OR NOTIFY.REQUEST OR NOTIFY.SET)
    DISALLOW (SETTINGS.DEFAULT) AND
		(SETTINGS.MAIL OR SETTINGS.NOMAIL OR
		 SETTINGS.REPRO OR SETTINGS.NOREPRO OR
		 SETTINGS.CONCEAL OR SETTINGS.NOCONCEAL OR
		 SETTINGS.DIGEST OR SETTINGS.NODIGEST OR
		 SETTINGS.POST OR SETTINGS.NOPOST)

DEFINE SYNTAX DEFFSUCMD
    ROUTINE CMD_DEFFSU
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=FSNAME, PROMPT="File server name", VALUE (REQUIRED)
    QUALIFIER MANAGER, NONNEGATABLE, VALUE (REQUIRED)
    QUALIFIER ROOT, NONNEGATABLE, VALUE (REQUIRED, TYPE=$FILE)
    QUALIFIER DELAY_THRESHOLD, NEGATABLE, VALUE (REQUIRED, TYPE=$NUMBER)
    QUALIFIER BEGIN_SEND_PERIOD, NONNEGATABLE, VALUE (REQUIRED, TYPE=$DATETIME)
    QUALIFIER END_SEND_PERIOD, NONNEGATABLE, VALUE (REQUIRED, TYPE=$DATETIME)
    QUALIFIER MAILING_LIST, NEGATABLE, VALUE (REQUIRED)
    QUALIFIER SERVER_LIMIT, NEGATABLE, VALUE (REQUIRED, TYPE=$NUMBER)
    QUALIFIER HOST_LIMIT, NEGATABLE, VALUE (REQUIRED, TYPE=$NUMBER)
    QUALIFIER USER_LIMIT, NEGATABLE, VALUE (REQUIRED, TYPE=$NUMBER)
    QUALIFIER DESCRIPTION, NEGATABLE, VALUE (REQUIRED)

DEFINE SYNTAX ADDUSER
    ROUTINE CMD_UA_ADD
    PARAMETER P1, LABEL=ADDKWD, PROMPT="ADD option", VALUE (REQUIRED, TYPE=ADDKWDS)
    PARAMETER P2, LABEL=USERNAME, VALUE (REQUIRED)
    QUALIFIER PASSWORD, NONNEGATABLE, VALUE (REQUIRED)

DEFINE SYNTAX MODUSER
    ROUTINE CMD_UA_MODIFY
    PARAMETER P1, LABEL=DEFKWD, PROMPT="MODIFY option", VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=USERNAME, VALUE (REQUIRED)
    QUALIFIER PASSWORD, NONNEGATABLE, VALUE (REQUIRED)

DEFINE SYNTAX REMUSER
    ROUTINE CMD_UA_REMOVE
    PARAMETER P1, LABEL=REMKWD, PROMPT="REMOVE option", VALUE (REQUIRED, TYPE=REMKWDS)
    PARAMETER P2, LABEL=USERNAME, VALUE (REQUIRED)

DEFINE SYNTAX CREUDB
    ROUTINE CMD_UA_CREDB
    PARAMETER P1, LABEL=CREKWD, PROMPT="CREATE option", VALUE (REQUIRED, TYPE=CREKWDS)
    PARAMETER P2, LABEL=FSPEC, VALUE (TYPE=$FILE)

DEFINE TYPE ADDKWDS
    KEYWORD USER, SYNTAX=ADDUSER, NONNEGATABLE

DEFINE TYPE CREKWDS
    KEYWORD USER_DATABASE, SYNTAX=CREUDB, NONNEGATABLE

DEFINE VERB ADD
    PARAMETER P1, LABEL=ADDKWD, PROMPT="ADD option", VALUE (REQUIRED, TYPE=ADDKWDS)

DEFINE VERB CREATE
    PARAMETER P1, LABEL=CREKWD, PROMPT="CREATE option", VALUE (REQUIRED, TYPE=CREKWDS)

DEFINE VERB DEFINE
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    QUALIFIER KEY, SYNTAX=DEFINE_KEY

DEFINE VERB MODIFY
    PARAMETER P1, LABEL=DEFKWD, PROMPT="MODIFY option",
    	VALUE (REQUIRED, TYPE=MODKWDS)

DEFINE SYNTAX REMREWRITE
    ROUTINE CMD_REMREW
    PARAMETER P1, LABEL=REMKWD, PROMPT="REMOVE option",
    	VALUE (REQUIRED, TYPE=REMKWDS)
    PARAMETER P2, LABEL=LHS, PROMPT="Left-hand side", VALUE (REQUIRED)

DEFINE SYNTAX REMLCLDOM
    ROUTINE CMD_REMLCLDOM
    PARAMETER P1, LABEL=DEFKWD, PROMPT="REMOVE option",
    	VALUE (REQUIRED, TYPE=REMKWDS)
    PARAMETER P2, LABEL=HOSTNM, PROMPT="domain name pattern", VALUE (REQUIRED)

DEFINE SYNTAX REMPATHCMD
    ROUTINE CMD_REMPATH
    PARAMETER P1, LABEL=REMKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=REMKWDS)
    PARAMETER P2, LABEL=DOMAIN, PROMPT="Domain", VALUE (REQUIRED)

DEFINE SYNTAX REMALIASCMD
    ROUTINE CMD_REMALIAS
    PARAMETER P1, LABEL=REMKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=REMKWDS)
    PARAMETER P2, LABEL=LCLNAME, PROMPT="Local name", VALUE (REQUIRED)

DEFINE SYNTAX REMLISTCMD
    ROUTINE CMD_REMLIST
    PARAMETER P1, LABEL=REMKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=REMKWDS)
    PARAMETER P2, LABEL=LISTNAME, PROMPT="List name", VALUE (REQUIRED)

DEFINE SYNTAX REMFSUCMD
    ROUTINE CMD_REMFSU
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=FSNAME, PROMPT="File server name", VALUE (REQUIRED)

DEFINE SYNTAX REMRELAY
    ROUTINE CMD_REMRELAY
    PARAMETER P1, LABEL=DEFKWD, PROMPT="DEFINE option",
    	VALUE (REQUIRED, TYPE=DEFKWDS)
    PARAMETER P2, LABEL=NETADDR, PROMPT="IP address", VALUE (REQUIRED)
    QUALIFIER NETMASK, VALUE (REQUIRED)

DEFINE VERB REMOVE
    PARAMETER P1, LABEL=REMKWD, PROMPT="REMOVE option",
    	VALUE (REQUIRED, TYPE=REMKWDS)

DEFINE VERB SAVE
    ROUTINE CMD_SAVE
    PARAMETER P1, LABEL=FILESPEC, VALUE (TYPE=$FILE)

DEFINE VERB EXIT
    ROUTINE CMD_EXIT

DEFINE VERB QUIT
    ROUTINE CMD_QUIT

DEFINE VERB HELP
    ROUTINE CMD_HELP
    PARAMETER P1, LABEL=HELP_REQUEST, VALUE (TYPE=$REST_OF_LINE)
    NOQUALIFIERS

DEFINE VERB SHOW
    ROUTINE CMD_SHOW
    SYNONYM SH
    PARAMETER P1, LABEL=SHOWOPT, VALUE (TYPE=SHOKWDS, REQUIRED, LIST),
    	PROMPT="SHOW option"
    PARAMETER P2, LABEL=PATTERN, VALUE (DEFAULT="*")
    QUALIFIER OUTPUT, VALUE (TYPE=$FILE, REQUIRED), NONNEGATABLE
    QUALIFIER COMMAND
    QUALIFIER BRIEF, NONNEGATABLE
    QUALIFIER FULL, NONNEGATABLE
    DISALLOW  ANY2 (COMMAND, BRIEF, FULL)

DEFINE VERB SET
    ROUTINE CMD_SET
    PARAMETER P1, LABEL=SETOPT, VALUE (TYPE=SETKWDS, REQUIRED),
    	PROMPT="SET option"

DEFINE TYPE SETKWDS
    KEYWORD SMTP, SYNTAX=SETSMTP
    KEYWORD LOCAL, SYNTAX=SETLOCAL
    KEYWORD ROUTER, SYNTAX=SETRTR
    KEYWORD DECNET_SMTP, SYNTAX=SETDNSMTP
    KEYWORD SITE, SYNTAX=SETSITE
    KEYWORD MLF, SYNTAX=SETMLF

DEFINE TYPE AUTHKWDS
    KEYWORD CRAM_MD5
    KEYWORD PLAIN

DEFINE SYNTAX SETSMTP
    ROUTINE CMD_SET
    PARAMETER P1, LABEL=SETOPT, VALUE (TYPE=SETKWDS, REQUIRED),
    	PROMPT="SET option"
    QUALIFIER RETRY_INTERVAL, NONNEGATABLE, VALUE (TYPE=$DELTATIME, REQUIRED)
