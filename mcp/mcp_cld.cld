DEFINE SYNTAX QUEUE_SHOW
    ROUTINE QCMD_SHOW
    PARAMETER P1, LABEL=QOPER, VALUE (TYPE=QVERB,REQUIRED), PROMPT="Operation"
    PARAMETER P2, LABEL=ENTNUM, VALUE (TYPE=$NUMBER,LIST)
    QUALIFIER BRIEF, NONNEGATABLE
    QUALIFIER FULL, NONNEGATABLE
    QUALIFIER ALL, NONNEGATABLE
    QUALIFIER SELECTED, NONNEGATABLE
    QUALIFIER WAITING, NONNEGATABLE
    QUALIFIER HELD, NONNEGATABLE
    QUALIFIER IN_PROGRESS, NONNEGATABLE
    QUALIFIER DATE, NONNEGATABLE
    QUALIFIER OUTPUT, VALUE (TYPE=$FILE, REQUIRED), NONNEGATABLE
    QUALIFIER CREATED, NONNEGATABLE
    QUALIFIER MODIFIED, NONNEGATABLE
    QUALIFIER DELAY, NONNEGATABLE
    QUALIFIER EXPIRE, NONNEGATABLE
    QUALIFIER ORIGIN_AGENT, NONNEGATABLE, VALUE(REQUIRED,TYPE=orgagents)
    QUALIFIER DESTINATION_AGENT, NONNEGATABLE, VALUE(REQUIRED,TYPE=destagents)
    !
    !  The following two qualifiers can't use "TODAY" because CLI$GET_VALUE
    !  just returns the default string to the program.  Instead, they use
    !  -- 00:00:00.00, which is "TODAY".
    !
    QUALIFIER SINCE, VALUE(DEFAULT="-- 00:00:00.00",TYPE=$DATETIME), NONNEGATABLE
    QUALIFIER BEFORE, VALUE(DEFAULT="-- 00:00:00.00",TYPE=$DATETIME), NONNEGATABLE
    DISALLOW ANY2 (CREATED, MODIFIED, DELAY, EXPIRE)
    DISALLOW BRIEF AND FULL
    DISALLOW ANY2 (IN_PROGRESS, ALL, WAITING, HELD)
    DISALLOW (DATE and FULL)
    DISALLOW SELECTED AND (ALL OR WAITING OR HELD OR IN_PROGRESS OR DATE OR CREATED OR
    	    	    MODIFIED OR DELAY OR EXPIRE OR ORIGIN_AGENT OR DESTINATION_AGENT OR ENTNUM)

DEFINE SYNTAX QUEUE_SELECT
    ROUTINE QCMD_SELECT
    PARAMETER P1, LABEL=QOPER, VALUE (TYPE=QVERB,REQUIRED), PROMPT="Operation"
    QUALIFIER WAITING, NONNEGATABLE
    QUALIFIER HELD, NONNEGATABLE
    QUALIFIER IN_PROGRESS, NONNEGATABLE
    QUALIFIER CREATED, NONNEGATABLE
    QUALIFIER MODIFIED, NONNEGATABLE
    QUALIFIER DELAY, NONNEGATABLE
    QUALIFIER EXPIRE, NONNEGATABLE
    QUALIFIER ORIGIN_AGENT, NONNEGATABLE, VALUE(REQUIRED,TYPE=orgagents)
    QUALIFIER DESTINATION_AGENT, NONNEGATABLE, VALUE(REQUIRED,TYPE=destagents)
    !
    !  The following two qualifiers can't use "TODAY" because CLI$GET_VALUE
    !  just returns the default string to the program.  Instead, they use
    !  -- 00:00:00.00, which is "TODAY".
    !
    QUALIFIER SINCE, VALUE(DEFAULT="-- 00:00:00.00",TYPE=$DATETIME), NONNEGATABLE
    QUALIFIER BEFORE, VALUE(DEFAULT="-- 00:00:00.00",TYPE=$DATETIME), NONNEGATABLE
    DISALLOW ANY2 (CREATED, MODIFIED, DELAY, EXPIRE)
    DISALLOW ANY2 (IN_PROGRESS, WAITING, HELD)

